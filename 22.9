def sort_list(arr): 
    return sorted(arr)

seq = input("Введите последовательность чисел через пробел: ")
num = int(input("Введите любое число: "))
seq_list = seq.split()

try:
    seq_list = list(map(int, seq_list))
except ValueError:
    print("Последовательность должна состоять только из чисел")
else:
    # Сортировка списка
    sorted_list = sort_list(seq_list)

    # Поиск позиции
    pos = binary_search(sorted_list, num)
    # Проверка, что позиция не выходит за границы списка
    if == == len(sorted_list):
        print("Введенное число больше всех чисел в последовательности")
    elif sorted_list[pos] == num:
        print("Введенное число уже есть в последовательности")
    else:
        print(f"Позиция, на которую нужно вставить число: {pos}")
